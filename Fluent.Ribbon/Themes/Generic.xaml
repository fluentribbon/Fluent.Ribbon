      <MultiTrigger>
        <MultiTrigger.Conditions>
          <Condition Property="Size" Value="Large" />
          <Condition SourceName="iconImage" Property="OptimalIcon" Value="{x:Null}" />
        </MultiTrigger.Conditions>
        <Setter TargetName="controlLabel" Property="Margin" Value="0" />
