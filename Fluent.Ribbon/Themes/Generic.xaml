          <StackPanel x:Name="screenTipPanel">
            <TextBlock x:Name="title" Margin="7 8 7 10" FontWeight="Bold" Text="{TemplateBinding Property=Title}" TextWrapping="Wrap" />
            <Grid x:Name="imageAndTextSection">
              <Grid.ColumnDefinitions>
