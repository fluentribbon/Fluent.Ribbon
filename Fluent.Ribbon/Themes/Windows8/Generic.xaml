              <Setter Property="Stroke" TargetName="path1" Value="{DynamicResource ButtonHoverBorderBrush}" />
              <Setter Property="Fill" TargetName="path" Value="{DynamicResource ButtonHoverBackgroundBrush}" />
              <Setter Property="Stroke" TargetName="path" Value="{DynamicResource ButtonHoverBorderBrush}" />
