        <Setter Property="Background" TargetName="outerBorder" Value="{DynamicResource ButtonHoverBackgroundBrush}" />
        <Setter Property="Background" TargetName="innerBorder" Value="{DynamicResource ButtonHoverBackgroundBrush}" />
        <Setter Property="BorderBrush" TargetName="innerBorder" Value="{DynamicResource ButtonHoverBorderBrush}" />
        <Setter Property="BorderBrush" TargetName="outerBorder" Value="{DynamicResource ButtonHoverBorderBrush}" />
