<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Fluent="clr-namespace:Fluent"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    mc:Ignorable="options d">
    <!-- Matadata -->
    <system:String x:Key="Theme.Name">{{ThemeName}}</system:String>
    <system:String x:Key="Theme.Origin">Fluent.Ribbon</system:String>
    <system:String x:Key="Theme.DisplayName">{{ThemeDisplayName}}</system:String>
    <system:String x:Key="Theme.BaseColorScheme">{{BaseColorScheme}}</system:String>
    <system:String x:Key="Theme.ColorScheme">{{ColorScheme}}</system:String>
    <system:String x:Key="Theme.AlternativeColorScheme">{{AlternativeColorScheme}}</system:String>

    <Color x:Key="Theme.PrimaryAccentColor">{{Fluent.Ribbon.Colors.AccentBase}}</Color>
    <SolidColorBrush x:Key="Theme.ShowcaseBrush" Color="{{Fluent.Ribbon.Colors.AccentBase}}" options:Freeze="True" />
    <system:Boolean x:Key="Theme.IsHighContrast">{{IsHighContrast}}</system:Boolean>

    <!-- COLORS -->

    <!-- Accent colors -->
    <Color x:Key="Fluent.Ribbon.Colors.AccentBase">{{Fluent.Ribbon.Colors.AccentBase}}</Color>
    <!-- 80% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent80">{{Fluent.Ribbon.Colors.Accent80}}</Color>
    <!-- 60% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent60">{{Fluent.Ribbon.Colors.Accent60}}</Color>
    <!-- 40% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent40">{{Fluent.Ribbon.Colors.Accent40}}</Color>
    <!-- 20% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent20">{{Fluent.Ribbon.Colors.Accent20}}</Color>

    <Color x:Key="Fluent.Ribbon.Colors.Highlight">{{Fluent.Ribbon.Colors.Highlight}}</Color>

    <Color x:Key="Fluent.Ribbon.Colors.AccentLight1">{{Fluent.Ribbon.Colors.AccentLight1}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.AccentLight1.Foreground">{{Fluent.Ribbon.Colors.AccentLight1.Foreground}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.AccentLight2">{{Fluent.Ribbon.Colors.AccentLight2}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.AccentLight2.Foreground">{{Fluent.Ribbon.Colors.AccentLight2.Foreground}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.AccentLight3">{{Fluent.Ribbon.Colors.AccentLight3}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.AccentLight3.Foreground">{{Fluent.Ribbon.Colors.AccentLight3.Foreground}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.AccentDark1">{{Fluent.Ribbon.Colors.AccentDark1}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.AccentDark1.Foreground">{{Fluent.Ribbon.Colors.AccentDark1.Foreground}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.AccentDark2">{{Fluent.Ribbon.Colors.AccentDark2}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.AccentDark2.Foreground">{{Fluent.Ribbon.Colors.AccentDark2.Foreground}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.AccentDark3">{{Fluent.Ribbon.Colors.AccentDark3}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.AccentDark3.Foreground">{{Fluent.Ribbon.Colors.AccentDark3.Foreground}}</Color>

    <!-- Base colors -->
    <Color x:Key="Fluent.Ribbon.Colors.Black">{{Fluent.Ribbon.Colors.Black}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Black20">{{Fluent.Ribbon.Colors.Black20}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.White">{{Fluent.Ribbon.Colors.White}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.White20">{{Fluent.Ribbon.Colors.White20}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray1">{{Fluent.Ribbon.Colors.Gray1}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray2">{{Fluent.Ribbon.Colors.Gray2}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray3">{{Fluent.Ribbon.Colors.Gray3}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray4">{{Fluent.Ribbon.Colors.Gray4}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray5">{{Fluent.Ribbon.Colors.Gray5}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray6">{{Fluent.Ribbon.Colors.Gray6}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray7">{{Fluent.Ribbon.Colors.Gray7}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray8">{{Fluent.Ribbon.Colors.Gray8}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray9">{{Fluent.Ribbon.Colors.Gray9}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray10">{{Fluent.Ribbon.Colors.Gray10}}</Color>

    <Color x:Key="Fluent.Ribbon.Colors.TransparentWhite">#00FFFFFF</Color>
    <Color x:Key="Fluent.Ribbon.Colors.HighTransparentWhite">#17FFFFFF</Color>

    <!-- Foreground -->
    <Color x:Key="Fluent.Ribbon.Colors.IdealForeground">{{Fluent.Ribbon.Colors.IdealForeground}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.DarkIdealForegroundDisabled">{{Fluent.Ribbon.Colors.DarkIdealForegroundDisabled}}</Color>

    <!-- Misc colors -->
    <Color x:Key="Fluent.Ribbon.Colors.ExtremeHighlight">{{Fluent.Ribbon.Colors.ExtremeHighlight}}</Color>
    <Color x:Key="Fluent.Ribbon.Colors.DarkExtremeHighlight">{{Fluent.Ribbon.Colors.DarkExtremeHighlight}}</Color>

    <!-- END COLORS -->

    <!-- BRUSHES -->

    <!-- Accent brushes -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentBase" Color="{StaticResource Fluent.Ribbon.Colors.AccentBase}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Accent80" Color="{StaticResource Fluent.Ribbon.Colors.Accent80}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Accent60" Color="{StaticResource Fluent.Ribbon.Colors.Accent60}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Accent40" Color="{StaticResource Fluent.Ribbon.Colors.Accent40}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Accent20" Color="{StaticResource Fluent.Ribbon.Colors.Accent20}" options:Freeze="True" />

    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentLight1" Color="{StaticResource Fluent.Ribbon.Colors.AccentLight1}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentLight1.Foreground" Color="{StaticResource Fluent.Ribbon.Colors.AccentLight1.Foreground}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentLight2" Color="{StaticResource Fluent.Ribbon.Colors.AccentLight2}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentLight2.Foreground" Color="{StaticResource Fluent.Ribbon.Colors.AccentLight2.Foreground}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentLight3" Color="{StaticResource Fluent.Ribbon.Colors.AccentLight3}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentLight3.Foreground" Color="{StaticResource Fluent.Ribbon.Colors.AccentLight3.Foreground}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentDark1" Color="{StaticResource Fluent.Ribbon.Colors.AccentDark1}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentDark1.Foreground" Color="{StaticResource Fluent.Ribbon.Colors.AccentDark1.Foreground}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentDark2" Color="{StaticResource Fluent.Ribbon.Colors.AccentDark2}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentDark2.Foreground" Color="{StaticResource Fluent.Ribbon.Colors.AccentDark2.Foreground}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentDark3" Color="{StaticResource Fluent.Ribbon.Colors.AccentDark3}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentDark3.Foreground" Color="{StaticResource Fluent.Ribbon.Colors.AccentDark3.Foreground}" options:Freeze="True" />

    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Highlight" Color="{StaticResource Fluent.Ribbon.Colors.Highlight}" options:Freeze="True" />

    <!-- Misc brushes -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.ExtremeHighlight" Color="{StaticResource Fluent.Ribbon.Colors.ExtremeHighlight}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.DarkExtremeHighlight" Color="{StaticResource Fluent.Ribbon.Colors.DarkExtremeHighlight}" options:Freeze="True" />

    <!-- Base brushes -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.White" Color="{StaticResource Fluent.Ribbon.Colors.White}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.White20" Color="{StaticResource Fluent.Ribbon.Colors.White20}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Black" Color="{StaticResource Fluent.Ribbon.Colors.Black}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Black20" Color="{StaticResource Fluent.Ribbon.Colors.Black20}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray1" Color="{StaticResource Fluent.Ribbon.Colors.Gray1}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray2" Color="{StaticResource Fluent.Ribbon.Colors.Gray2}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray3" Color="{StaticResource Fluent.Ribbon.Colors.Gray3}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray4" Color="{StaticResource Fluent.Ribbon.Colors.Gray4}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray5" Color="{StaticResource Fluent.Ribbon.Colors.Gray5}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray6" Color="{StaticResource Fluent.Ribbon.Colors.Gray6}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray7" Color="{StaticResource Fluent.Ribbon.Colors.Gray7}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray8" Color="{StaticResource Fluent.Ribbon.Colors.Gray8}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray9" Color="{StaticResource Fluent.Ribbon.Colors.Gray9}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray10" Color="{StaticResource Fluent.Ribbon.Colors.Gray10}" options:Freeze="True" />

    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.TransparentWhite" Color="{StaticResource Fluent.Ribbon.Colors.TransparentWhite}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.HighTransparentWhite" Color="{StaticResource Fluent.Ribbon.Colors.HighTransparentWhite}" options:Freeze="True" />

    <!-- Foreground -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.IdealForeground" Color="{StaticResource Fluent.Ribbon.Colors.IdealForeground}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.IdealForegroundDisabled" Color="{StaticResource Fluent.Ribbon.Colors.IdealForeground}" Opacity="0.4" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.DarkIdealForegroundDisabled" Color="{StaticResource Fluent.Ribbon.Colors.DarkIdealForegroundDisabled}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.LabelText" Color="{{Fluent.Ribbon.Brushes.LabelText}}" options:Freeze="True" />

    <!-- Generic control brushes -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Control.Border" Color="{StaticResource Fluent.Ribbon.Colors.Gray6}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Control.Disabled.Border" Color="{StaticResource Fluent.Ribbon.Colors.Gray5}" options:Freeze="True" />

    <!-- Backstage -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Backstage.Background" Color="{{Fluent.Ribbon.Brushes.Backstage.Background}}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Backstage.Foreground" Color="{{Fluent.Ribbon.Brushes.Backstage.Foreground}}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Backstage.BackButton.Background" Color="{{Fluent.Ribbon.Brushes.Backstage.BackButton.Background}}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Backstage.BackButton.Foreground" Color="{{Fluent.Ribbon.Brushes.Backstage.BackButton.Foreground}}" options:Freeze="True" />

    <!-- BackstageTabControl -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.BackstageTabControl.Background" Color="{{Fluent.Ribbon.Brushes.BackstageTabControl.Background}}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.BackstageTabControl.Button.MouseOver.Background" Color="{{Fluent.Ribbon.Brushes.BackstageTabControl.Button.MouseOver.Background}}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.BackstageTabControl.ItemsPanelBackground" Color="{{Fluent.Ribbon.Brushes.BackstageTabControl.ItemsPanelBackground}}" options:Freeze="True" />

    <!-- BackstageTabItem -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.BackstageTabItem.Header.Foreground" Color="{{Fluent.Ribbon.Brushes.BackstageTabItem.Header.Foreground}}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.BackstageTabItem.MouseOver.Background" Color="{{Fluent.Ribbon.Brushes.BackstageTabItem.MouseOver.Background}}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.BackstageTabItem.Selected.Background" Color="{{Fluent.Ribbon.Brushes.BackstageTabItem.Selected.Background}}" options:Freeze="True" />

    <!-- Button -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Button.MouseOver.Border" Color="{StaticResource Fluent.Ribbon.Colors.Accent40}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Button.MouseOver.Background" Color="{StaticResource Fluent.Ribbon.Colors.Accent20}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Button.Pressed.Border" Color="{StaticResource Fluent.Ribbon.Colors.Accent60}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Button.Pressed.Background" Color="{StaticResource Fluent.Ribbon.Colors.Accent40}" options:Freeze="True" />

    <!-- ToggleButton -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.ToggleButton.Checked.Background" Color="{StaticResource Fluent.Ribbon.Colors.Accent20}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.ToggleButton.Checked.Border" Color="{StaticResource Fluent.Ribbon.Colors.Highlight}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.ToggleButton.CheckedMouseOver.Background" Color="{StaticResource Fluent.Ribbon.Colors.Accent20}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.ToggleButton.CheckedMouseOver.Border" Color="{StaticResource Fluent.Ribbon.Colors.Accent60}" options:Freeze="True" />

    <!-- CheckBox -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.CheckBox.Border" Color="{StaticResource Fluent.Ribbon.Colors.Black}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.CheckBox.Background" Color="Transparent" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.CheckBox.MouseOver.Background" Color="Transparent" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.CheckBox.MouseOver.Stroke" Color="{StaticResource Fluent.Ribbon.Colors.Gray1}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.CheckBox.Pressed.Stroke" Color="{StaticResource Fluent.Ribbon.Colors.Black}" options:Freeze="True" />

    <!-- ColorGallery -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.ColorGallery.Background" Color="{StaticResource Fluent.Ribbon.Colors.White}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.ColorGallery.Item.Border" Color="{StaticResource Fluent.Ribbon.Colors.Gray8}" options:Freeze="True" />

    <!-- DropDown -->
